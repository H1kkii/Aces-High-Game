### CÃ³digo Menu Principal


Menu Principal

using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.SceneManagement;

public class MenuPrincipalManager : MonoBehaviour
{
    [SerializeField] private string Espaco;
    [SerializeField] private string Cidade;
    [SerializeField] private string Fazenda;

    public void jogarEspaco()
    {
        SceneManager.LoadScene(Espaco);
    }

    public void jogarCidade()
    {
        SceneManager.LoadScene(Cidade);
    }

    public void jogarFazenda()
    {
        SceneManager.LoadScene(Fazenda);
    }
}
